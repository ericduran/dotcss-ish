{
  "name" : "dotcss-ish",
  "manifest_version" : 2,
  "version" : "1.0.0",
  "description" : "~/.css",
  "icons": { "48" : "icon48.png", "128" : "icon128.png" },
  "content_scripts": [{
    "js" : ["dotcss-ish.js"],
    "matches" : ["http://*/*", "https://*/*"],
    "run_at" : "document_end"
  }],
  "permissions": ["tabs"]
}
